      <bind calculate="../inputs/contact/date_of_birth" nodeset="/immunization/patient_date_of_birth" type="string"/>
      <bind calculate="floor( difference-in-months(  /immunization/patient_date_of_birth , today() ) div 12 )" nodeset="/immunization/patient_age_in_years" type="string"/>
      <bind calculate="difference-in-months(  /immunization/patient_date_of_birth , today() )" nodeset="/immunization/patient_age_in_months" type="string"/>
      <bind calculate="floor((decimal-date-time(today()) - decimal-date-time( /immunization/patient_date_of_birth )) div 7)" nodeset="/immunization/patient_age_in_weeks" type="string"/>
      <bind calculate="floor( decimal-date-time(today()) - decimal-date-time( /immunization/patient_date_of_birth ) )" nodeset="/immunization/patient_age_in_days" type="string"/>
      <bind calculate="../vaccines_given/received_vaccines" nodeset="/immunization/received_vaccines" type="string"/>
      <bind calculate="concat(if(selected(../vaccines_given/vaccines, 'bcg'),'bcg',''), ' ',if(selected(../vaccines_given/vaccines, 'birth_polio'),'birth_polio',''), ' ',if(selected(../vaccines_given/vaccines, 'vitamin_a'),'vitamin_a',''), ' ',if(selected(../vaccines_given/vaccines, 'ipv'),'ipv',''), ' ',../vaccines_given/opv, ' ', ../vaccines_given/pcv, ' ', ../vaccines_given/dpt_hepb_hib, ' ', ../vaccines_given/rota, ' ', ../vaccines_given/measles)" nodeset="/immunization/c_received_vaccines" relevant="selected(../vaccines_given/received_vaccines, 'yes')" type="string"/>
      <bind calculate="concat( /immunization/received_bcg ,  /immunization/received_birth_polio ,  /immunization/received_opv ,  /immunization/received_pcv ,  /immunization/received_dpts ,  /immunization/received_ipv ,  /immunization/received_rota ,  /immunization/received_vitamin_a ,  /immunization/received_measles )" nodeset="/immunization/received_vaccines_detail" relevant="selected(../vaccines_given/received_vaccines, 'yes')" type="string"/>
      <bind calculate="&quot;BCG, &quot;" nodeset="/immunization/received_bcg" relevant="selected(../vaccines_given/vaccines, 'bcg')" type="string"/>
      <bind calculate="&quot;Birth Polio (OPV 0), &quot;" nodeset="/immunization/received_birth_polio" relevant="selected(../vaccines_given/vaccines, 'birth_polio')" type="string"/>
      <bind calculate="if(../vaccines_given/opv=&quot;opv_1&quot;, &quot;OPV 1, &quot;,if(../vaccines_given/rota=&quot;opv_2&quot;, &quot;OPV 2, &quot;,if(../vaccines_given/opv=&quot;opv_3&quot;, &quot;OPV 3, &quot;,'' )))" nodeset="/immunization/received_opv" relevant="selected(../vaccines_given/vaccines, 'opv')" type="string"/>
      <bind calculate="if(../vaccines_given/pcv=&quot;pcv_1&quot;, &quot;PCV 1, &quot;,if(../vaccines_given/rota=&quot;pcv_2&quot;, &quot;PCV 2, &quot;,if(../vaccines_given/pcv=&quot;pcv_3&quot;, &quot;PCV 3, &quot;,'' )))" nodeset="/immunization/received_pcv" relevant="selected(../vaccines_given/vaccines, 'pcv')" type="string"/>
      <bind calculate="if(../vaccines_given/dpt_hepb_hib=&quot;dpt_hepb_hib_1&quot;, &quot;DPT-HepB-Hib 1, &quot;,if(../vaccines_given/dpt_hepb_hib=&quot;dpt_hepb_hib_2&quot;, &quot;DPT-HepB-Hib 2, &quot;,if(../vaccines_given/dpt_hepb_hib=&quot;dpt_hepb_hib_3&quot;, &quot;DPT-HepB-Hib 3, &quot;,'' )))" nodeset="/immunization/received_dpts" relevant="selected(../vaccines_given/vaccines, 'dpt_hepb_hib')" type="string"/>
      <bind calculate="&quot;IPV, &quot;" nodeset="/immunization/received_ipv" relevant="selected(../vaccines_given/vaccines, 'ipv')" type="string"/>
      <bind calculate="if(../vaccines_given/rota=&quot;rota_1&quot;, &quot;Rota 1, &quot;,if(../vaccines_given/rota=&quot;rota_2&quot;, &quot;Rota 2, &quot;,'' ))" nodeset="/immunization/received_rota" relevant="selected(../vaccines_given/vaccines, 'rota')" type="string"/>
      <bind calculate="&quot;Vitamin A, &quot;" nodeset="/immunization/received_vitamin_a" relevant="selected(../vaccines_given/vaccines, 'vitamin_a')" type="string"/>
      <bind calculate="if(../vaccines_given/measles=&quot;measles_1&quot;, &quot;Measles 1, &quot;,if(../vaccines_given/measles=&quot;measles_2&quot;, &quot;Measles 2, &quot;,'' ))" nodeset="/immunization/received_measles" relevant="selected(../vaccines_given/vaccines, 'measles')" type="string"/>
      <bind calculate=" /immunization/vaccines_given/next_visit " nodeset="/immunization/c_next_visit" relevant="selected(../vaccines_given/received_vaccines, 'yes')" type="string"/>
      <bind nodeset="/immunization/vaccines_given/received_vaccines" required="true()" type="select1"/>
      <bind nodeset="/immunization/vaccines_given/n_advise_parent" readonly="true()" relevant="selected(../received_vaccines, 'no')" type="string"/>
      <bind constraint="decimal-date-time(.) &lt;= decimal-date-time(today())" jr:constraintMsg="jr:itext('/immunization/vaccines_given/vaccine_date:jr:constraintMsg')" nodeset="/immunization/vaccines_given/vaccine_date" relevant="selected(../received_vaccines, 'yes')" required="true()" type="date"/>
      <bind nodeset="/immunization/vaccines_given/vaccines" relevant="selected(../received_vaccines, 'yes')" required="true()" type="select"/>
      <bind nodeset="/immunization/vaccines_given/opv" relevant="selected(../vaccines, 'opv')" required="true()" type="select1"/>
      <bind nodeset="/immunization/vaccines_given/pcv" relevant="selected(../vaccines, 'pcv')" required="true()" type="select1"/>
      <bind nodeset="/immunization/vaccines_given/dpt_hepb_hib" relevant="selected(../vaccines, 'dpt_hepb_hib')" required="true()" type="select1"/>
      <bind nodeset="/immunization/vaccines_given/rota" relevant="selected(../vaccines, 'rota')" required="true()" type="select1"/>
      <bind nodeset="/immunization/vaccines_given/measles" relevant="selected(../vaccines, 'measles')" required="true()" type="select1"/>
      <bind constraint="decimal-date-time(.) &gt;= decimal-date-time(today())" jr:constraintMsg="jr:itext('/immunization/vaccines_given/next_visit:jr:constraintMsg')" nodeset="/immunization/vaccines_given/next_visit" relevant="selected(../received_vaccines, 'yes')" required="true()" type="date"/>
      <bind nodeset="/immunization/group_review/n_patient_details_title" readonly="true()" type="string"/>
      <bind nodeset="/immunization/group_review/n_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/immunization/group_review/n_vaccines_title" readonly="true()" type="string"/>
      <bind nodeset="/immunization/group_review/n_not_vaccinated" readonly="true()" relevant="selected(../../vaccines_given/received_vaccines, 'no')" type="string"/>
      <bind nodeset="/immunization/group_review/n_bcg" readonly="true()" relevant="selected(../../vaccines_given/vaccines, 'bcg')" type="string"/>
      <bind nodeset="/immunization/group_review/n_opv0" readonly="true()" relevant="selected(../../vaccines_given/vaccines, 'birth_polio')" type="string"/>
      <bind nodeset="/immunization/group_review/n_opv1" readonly="true()" relevant="selected(../../vaccines_given/opv, 'opv_1')" type="string"/>
      <bind nodeset="/immunization/group_review/n_opv2" readonly="true()" relevant="selected(../../vaccines_given/opv, 'opv_2')" type="string"/>
      <bind nodeset="/immunization/group_review/n_opv3" readonly="true()" relevant="selected(../../vaccines_given/opv, 'opv_3')" type="string"/>
      <bind nodeset="/immunization/group_review/n_pcv1" readonly="true()" relevant="selected(../../vaccines_given/pcv, 'pcv_1')" type="string"/>
      <bind nodeset="/immunization/group_review/n_pcv2" readonly="true()" relevant="selected(../../vaccines_given/pcv, 'pcv_2')" type="string"/>
      <bind nodeset="/immunization/group_review/n_pcv3" readonly="true()" relevant="selected(../../vaccines_given/pcv, 'pcv_3')" type="string"/>
      <bind nodeset="/immunization/group_review/n_dpt_hepb_hib1" readonly="true()" relevant="selected(../../vaccines_given/dpt_hepb_hib, 'dpt_hepb_hib_1')" type="string"/>
      <bind nodeset="/immunization/group_review/n_dpt_hepb_hib2" readonly="true()" relevant="selected(../../vaccines_given/dpt_hepb_hib, 'dpt_hepb_hib_2')" type="string"/>
      <bind nodeset="/immunization/group_review/n_dpt_hepb_hib3" readonly="true()" relevant="selected(../../vaccines_given/dpt_hepb_hib, 'dpt_hepb_hib_3')" type="string"/>
      <bind nodeset="/immunization/group_review/n_ipv" readonly="true()" relevant="selected(../../vaccines_given/vaccines, 'ipv')" type="string"/>
      <bind nodeset="/immunization/group_review/n_rota1" readonly="true()" relevant="selected(../../vaccines_given/rota, 'rota_1')" type="string"/>
      <bind nodeset="/immunization/group_review/n_rota2" readonly="true()" relevant="selected(../../vaccines_given/rota, 'rota_2')" type="string"/>
      <bind nodeset="/immunization/group_review/n_vitamin" readonly="true()" relevant="selected(../../vaccines_given/vaccines, 'vitamin_a')" type="string"/>
      <bind nodeset="/immunization/group_review/n_measles1" readonly="true()" relevant="selected(../../vaccines_given/measles, 'measles_1')" type="string"/>
      <bind nodeset="/immunization/group_review/n_measles2" readonly="true()" relevant="selected(../../vaccines_given/measles, 'measles_2')" type="string"/>
      <bind nodeset="/immunization/group_review/n_follow_up_title" readonly="true()" type="string"/>
      <bind nodeset="/immunization/group_review/n_follow_up_yes" readonly="true()" relevant="selected(../../vaccines_given/received_vaccines, 'yes')" type="string"/>
      <bind nodeset="/immunization/group_review/n_follow_up_no" readonly="true()" relevant="selected(../../vaccines_given/received_vaccines, 'no')" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/immunization/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/immunization/inputs">
      <label ref="jr:itext('/immunization/inputs:label')"/>
      <group ref="/immunization/inputs/contact">
        <label ref="jr:itext('/immunization/inputs/contact:label')"/>
        <input appearance="db-object" ref="/immunization/inputs/contact/_id">
          <label ref="jr:itext('/immunization/inputs/contact/_id:label')"/>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/immunization/vaccines_given">
      <label ref="jr:itext('/immunization/vaccines_given:label')"/>
      <select1 ref="/immunization/vaccines_given/received_vaccines">
        <label ref="jr:itext('/immunization/vaccines_given/received_vaccines:label')"/>
        <hint ref="jr:itext('/immunization/vaccines_given/received_vaccines:hint')"/>
        <item>
          <label ref="jr:itext('/immunization/vaccines_given/received_vaccines/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/immunization/vaccines_given/received_vaccines/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/immunization/vaccines_given/n_advise_parent">
        <label ref="jr:itext('/immunization/vaccines_given/n_advise_parent:label')"/>
      </input>
      <input ref="/immunization/vaccines_given/vaccine_date">
        <label ref="jr:itext('/immunization/vaccines_given/vaccine_date:label')"/>
      </input>
      <select ref="/immunization/vaccines_given/vaccines">
        <label ref="jr:itext('/immunization/vaccines_given/vaccines:label')"/>
        <itemset nodeset="instance('vaccines')/root/item[age_lower &lt;  /immunization/patient_age_in_weeks 
and age_upper &gt;  /immunization/patient_age_in_weeks  and contains(instance('contact-summary')/context/vaccines_not_received, name)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select>
      <select1 ref="/immunization/vaccines_given/opv">
        <label ref="jr:itext('/immunization/vaccines_given/opv:label')"/>
        <itemset nodeset="instance('polios')/root/item[age_lower &lt;  /immunization/patient_age_in_weeks 
and age_upper &gt;  /immunization/patient_age_in_weeks  and contains(instance('contact-summary')/context/vaccines_not_received, name)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/immunization/vaccines_given/pcv">
        <label ref="jr:itext('/immunization/vaccines_given/pcv:label')"/>
        <itemset nodeset="instance('pcvs')/root/item[age_lower &lt;=  /immunization/patient_age_in_weeks 
and age_upper &gt;  /immunization/patient_age_in_weeks  and contains(instance('contact-summary')/context/vaccines_not_received, name)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/immunization/vaccines_given/dpt_hepb_hib">
        <label ref="jr:itext('/immunization/vaccines_given/dpt_hepb_hib:label')"/>
        <itemset nodeset="instance('dpts')/root/item[age_lower &lt;=  /immunization/patient_age_in_weeks 
and age_upper &gt;  /immunization/patient_age_in_weeks  and contains(instance('contact-summary')/context/vaccines_not_received, name)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/immunization/vaccines_given/rota">
        <label ref="jr:itext('/immunization/vaccines_given/rota:label')"/>
        <itemset nodeset="instance('rotas')/root/item[age_lower &lt;=  /immunization/patient_age_in_weeks 
and age_upper &gt;  /immunization/patient_age_in_weeks  and contains(instance('contact-summary')/context/vaccines_not_received, name)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/immunization/vaccines_given/measles">
        <label ref="jr:itext('/immunization/vaccines_given/measles:label')"/>
        <itemset nodeset="instance('measles')/root/item[age_lower &lt;=  /immunization/patient_age_in_weeks 
and age_upper &gt;  /immunization/patient_age_in_weeks  and contains(instance('contact-summary')/context/vaccines_not_received, name)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/immunization/vaccines_given/next_visit">
        <label ref="jr:itext('/immunization/vaccines_given/next_visit:label')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/immunization/group_review">
      <input appearance="h1 yellow" ref="/immunization/group_review/n_patient_details_title">
        <label ref="jr:itext('/immunization/group_review/n_patient_details_title:label')"/>
      </input>
      <input ref="/immunization/group_review/n_patient_details">
        <label ref="jr:itext('/immunization/group_review/n_patient_details:label')"/>
      </input>
      <input appearance="h1 blue" ref="/immunization/group_review/n_vaccines_title">
        <label ref="jr:itext('/immunization/group_review/n_vaccines_title:label')"/>
      </input>
      <input ref="/immunization/group_review/n_not_vaccinated">
        <label ref="jr:itext('/immunization/group_review/n_not_vaccinated:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_bcg">
        <label ref="jr:itext('/immunization/group_review/n_bcg:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_opv0">
        <label ref="jr:itext('/immunization/group_review/n_opv0:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_opv1">
        <label ref="jr:itext('/immunization/group_review/n_opv1:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_opv2">
        <label ref="jr:itext('/immunization/group_review/n_opv2:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_opv3">
        <label ref="jr:itext('/immunization/group_review/n_opv3:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_pcv1">
        <label ref="jr:itext('/immunization/group_review/n_pcv1:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_pcv2">
        <label ref="jr:itext('/immunization/group_review/n_pcv2:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_pcv3">
        <label ref="jr:itext('/immunization/group_review/n_pcv3:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_dpt_hepb_hib1">
        <label ref="jr:itext('/immunization/group_review/n_dpt_hepb_hib1:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_dpt_hepb_hib2">
        <label ref="jr:itext('/immunization/group_review/n_dpt_hepb_hib2:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_dpt_hepb_hib3">
        <label ref="jr:itext('/immunization/group_review/n_dpt_hepb_hib3:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_ipv">
        <label ref="jr:itext('/immunization/group_review/n_ipv:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_rota1">
        <label ref="jr:itext('/immunization/group_review/n_rota1:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_rota2">
        <label ref="jr:itext('/immunization/group_review/n_rota2:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_vitamin">
        <label ref="jr:itext('/immunization/group_review/n_vitamin:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_measles1">
        <label ref="jr:itext('/immunization/group_review/n_measles1:label')"/>
      </input>
      <input appearance="li" ref="/immunization/group_review/n_measles2">
        <label ref="jr:itext('/immunization/group_review/n_measles2:label')"/>
      </input>
      <input appearance="h1 green" ref="/immunization/group_review/n_follow_up_title">
        <label ref="jr:itext('/immunization/group_review/n_follow_up_title:label')"/>
      </input>
      <input ref="/immunization/group_review/n_follow_up_yes">
        <label ref="jr:itext('/immunization/group_review/n_follow_up_yes:label')"/>
      </input>
      <input ref="/immunization/group_review/n_follow_up_no">
        <label ref="jr:itext('/immunization/group_review/n_follow_up_no:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
